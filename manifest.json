{
    "manifest_version": 3,
    "name": "Coding Contests",
    "version": "1.0",
    "description": "Displays upcoming coding contests.",
    
    "action": {
      "default_popup": "popup.html"
      }
    ,
    "permissions": [
      "activeTab",
      "storage"
    ]
,    "web_accessible_resources": [
      {
        "resources": ["*"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  